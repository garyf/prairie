<%= render partial: 'common/nav_cxt_solo', locals: {str: 'New numeric field'} %>
<%= render partial: 'common/link_to_field_set', locals: {o: @field_set} %>

<%= form_for(@numeric_field,
  url: setup_numeric_fields_path(@numeric_field),
  html: {class: 'form-horizontal'}) do |f| %>

  <%= f.hidden_field :field_set_id %>

  <%= render partial: 'form', locals: {f: f} %>

<% end %>
