<h4>New numeric field</h4>

<%= render partial: 'common/field_set_name', locals: {o: @field_set} %>

<%= form_for(@numeric_field,
  url: setup_numeric_fields_path(@numeric_field),
  html: {class: 'form-horizontal'}) do |f| %>

  <%= f.hidden_field :field_set_id %>

  <%= render partial: 'form', locals: {f: f} %>

<% end %>
