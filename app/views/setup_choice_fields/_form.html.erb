<%= render 'common/form_errors', o: @choice_field %>

<div class=control-group>
  <%= f.label :name, class: 'control-label' %>
  <div class=controls><%= f.text_field :name %></div>
</div>

<% if @row_edit_able_p %>
  <%= render partial: 'common/row_edit', locals: {f: f, rankable: @choice_field} %>
<% end %>

<div class=form-actions>
  <%= f.button 'Done', class: 'btn  btn-primary' %>
  <%= @field_set.btn_to_cancel_field %>
</div>
