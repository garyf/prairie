<h4>New string field</h4>

<%= render partial: 'common/link_to_field_set', locals: {o: @field_set} %>

<%= form_for(@string_field,
  url: setup_string_fields_path(@string_field),
  html: {class: 'form-horizontal'}) do |f| %>

  <%= f.hidden_field :field_set_id %>

  <%= render partial: 'form', locals: {f: f} %>

<% end %>
