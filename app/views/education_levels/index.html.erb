<h1>Listing education_levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Row</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @education_levels.each do |education_level| %>
      <tr>
        <td><%= education_level.name %></td>
        <td><%= education_level.description %></td>
        <td><%= education_level.row %></td>
        <td><%= link_to 'Show', education_level %></td>
        <td><%= link_to 'Edit', edit_education_level_path(education_level) %></td>
        <td><%= link_to 'Destroy', education_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education level', new_education_level_path %>
